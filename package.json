{
  "name": "typst--",
  "displayName": "typst++",
  "description": "Export Inline Formula to SVG",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "typst--.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "typst--.svg-export",
        "title": "TypstSvgExport"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "typst--.svg-export",
          "when": "editorLangId == typst",
          "group": "1_modification@1"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.25.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}